    "sinon": "^14.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/semver": "^7.3.9",
    "@types/sinon": "^10.0.11",
    "dotenv": "^16.0.1",
    "mocha": "^9.2.1",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  }
